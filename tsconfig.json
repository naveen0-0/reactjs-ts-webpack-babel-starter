      "module": "CommonJs",
      "jsx": "react",
      "sourceMap": true,
      "outDir": "./dist",
      "removeComments": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "preserveConstEnums": true
